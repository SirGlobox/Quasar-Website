{
  "name": "quasar-consultants-website",
  "version": "1.0.0",
  "description": "High-performance, SEO-optimized website for Quasar Consultants - Home Service Business Consulting in Charlotte, NC",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install && cd ../server && npm install",
    "start": "cd server && npm start"
  },
  "keywords": [
    "home service consulting",
    "Charlotte NC",
    "business consulting",
    "quasar consultants",
    "local business"
  ],
  "author": "Quasar Consultants",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "ogl": "^1.0.11",
    "react-scripts": "^5.0.1"
  }
}
